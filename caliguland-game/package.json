{
  "name": "caliguland-game",
  "version": "0.1.0",
  "description": "ERC-8004 Prediction Game - VibeVM Edition",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "bun run src/index.ts",
    "dev:blockchain": "ENABLE_BLOCKCHAIN=true bun run src/index.ts",
    "dev:fast": "TEST_MODE=true GAME_DURATION=300 bun run src/index.ts",
    "watch": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "ENABLE_BLOCKCHAIN=true jest src/__tests__/e2e --runInBand",
    "test:e2e:fast": "TEST_MODE=true ENABLE_BLOCKCHAIN=true jest src/__tests__/e2e --runInBand"
  },
  "keywords": [
    "caliguland",
    "erc-8004",
    "prediction-market",
    "tee",
    "game"
  ],
  "author": "Phala Network",
  "license": "Apache-2.0",
  "dependencies": {
    "@phala/dstack-sdk": "^0.1.0",
    "cors": "^2.8.5",
    "ethers": "^6.9.2",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
