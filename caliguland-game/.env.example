# Caliguland Game Configuration Example
# Copy this file to .env.local and update with your values

# Blockchain Configuration
ENABLE_BLOCKCHAIN=false
RPC_URL=http://localhost:8545
CHAIN_ID=1337
PRIVATE_KEY=your_private_key_here

# Contract Addresses (update after deployment)
JEJU_MARKET_ADDRESS=0x0000000000000000000000000000000000000000
ELIZA_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000
PREDICTION_ORACLE_ADDRESS=0x0000000000000000000000000000000000000000
REPUTATION_REGISTRY_ADDRESS=0x0000000000000000000000000000000000000000

# Game Configuration
GAME_DURATION=1800
MIN_PLAYERS=2
MAX_PLAYERS=20
STARTING_BALANCE=1000

# Server Configuration
PORT=3000
WEBSOCKET_PORT=3001
ENABLE_CORS=true

# Feature Flags
ENABLE_NPC_AI=false
ENABLE_TEE=true
DEBUG=false

# Testing
TEST_MODE=false
AUTO_FUND_ACCOUNTS=false
